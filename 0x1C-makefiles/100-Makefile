CC = gcc
SRC = main.c school.c

# a list of all SRC
OBJ = $(SRC:.c=.o)
NAME = school

# Remove program
RM = rm

# fav compiler flags
CFLAGS = -Wall -Werror -Wextra -pedantic

all : m.h $(OBJ)
	$(CC) $(CFLAGS) $(OBJ) -o $(NAME)

clean:
	$(RM) *~ $(NAME)

oclean:
	$(RM) $(OBJ)

fclean: clean oclean

re: fclean all
